pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
-- animation viewer
-- by rest


function _init()
	-- mouse devkit mode
	poke(0x5f2d, 1)
	t=0
end


function update60()

end


function _draw()
	cls()
	map()
	
	page=1
	for i=0,63 do
		spr(i*page,(i%16)*8,(flr(i/16)+5)*8)
	end
	
	if mouse_y>=40 and
				mouse_y<72 then
		local x1=mouse_x-(mouse_x%8)
		local y1=mouse_y-(mouse_y%8)
		rect(x1-1,y1-1,x1+8,y1+8,7)
	end
	
	
	spr(255,mouse_x,mouse_y)
	
	
	spr(255,stat(32)-1,stat(33)-1)
	print(stat(34))
end
